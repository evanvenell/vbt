
@{
    ViewBag.Title = "PageDevelopment";
}

<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">Error: Page Under Construction</h1>
                <h5 class="m-0 text-dark" style="padding-top: 20px;">@ViewBag.Message</h5>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="/Home/Index">Dashboard</a></li>
                    <li class="breadcrumb-item active">Page Under Development</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
        <!-- Row Spacer -->
        <div class="row mb-2">
            <div class="col-md-12">
                
            </div>
        </div>
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->
<!-- Main content -->
<section class="content">
    <div class="error-page">
        
            <div class="headline text-warning">Error</div>
            
       
            <div class="error-content">
                <div class="col-lg-12" style="padding-left: 30px;">
                    <h4>
                        <i class="fas fa-exclamation-triangle text-warning"></i>
                        We're Sorry, the page you are looking for is currently still under development at this time.
                    </h4>
                    <p>
                        If you are interested in a code walkthrough or
                        are experiencing technicial diffculties, please email <strong>evanvenell117@iamevanvenell.com</strong> and one of our team membors will
                        be happy to assist you. Otherwise, you may <a href="/Home/Index">return to the dashboard</a> whenever you are ready. We apologize
                        for this inconvenience and appreciate your pateince during this time.
                    </p>

                </div>

            </div>
        
        
    </div>
</section>


